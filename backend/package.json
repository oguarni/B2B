{
  "name": "conexhub-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seed.js",
    "test": "jest",
    "validate-config": "node scripts/validate-config.js",
    "generate-secrets": "node scripts/validate-config.js --generate",
    "prestart": "npm run validate-config",
    "security-check": "node scripts/validate-config.js && echo 'Security check passed'",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:prod": "NODE_ENV=production npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "build": "echo 'Backend build completed'",
    "postinstall": "echo 'Backend dependencies installed'"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.3",
    "sequelize": "^6.35.1",
    "sqlite3": "^5.1.7",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
}
